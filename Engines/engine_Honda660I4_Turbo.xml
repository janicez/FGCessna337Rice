<?xml version="1.0"?>

<piston_engine name="overboosted motorbike engine">
	<minmp unit="INHG">		    10.0  </minmp>
	<maxmp unit="INHG">		   28.0  </maxmp>
	<displacement unit="CC">  14755.0</displacement>
	<cylinders>					4.0  </cylinders>
	<compression-ratio>			26  </compression-ratio>
	<maxhp> 				  402.0  </maxhp>
	<cycles>				  4.0  </cycles>
	<idlerpm>				  803.0  </idlerpm>
	<maxrpm>				14000.0  </maxrpm>
	<sparkfaildrop>				0.1  </sparkfaildrop>
	<volumetric-efficiency>		1.10 </volumetric-efficiency>
	<air-intake-impedance-factor>0.05</air-intake-impedance-factor>
	<numboostspeeds>1</numboostspeeds>
	<boost-loss-factor>0.99</boost-loss-factor> <!-- It's a turbo, not a goddamned super. This should probably be lower(!)-->
	<ratedboost1>        25 </ratedboost1>
	<ratedpower1>       235 </ratedpower1>
	<ratedrpm1>       11500 </ratedrpm1>
	<ratedaltitude1>  47000 </ratedaltitude1>
	<man-press-lag>    0.05 </man-press-lag> <!-- to simulate turbo lag -->
	<boostmanual>         0 </boostmanual>
	<!-- Optional items that affect engine performance -->
	<!-- Defining <bsfc> over-rides the built-in horsepower calculations -->
	<bsfc>						0.33</bsfc>
	<!-- <bsfc>						0.326</bsfc> -->
</piston_engine>
